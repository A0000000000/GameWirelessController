<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CrossKey">
        <attr name="ck_type" format="integer"/>
        <attr name="ck_back_color" format="color"/>
        <attr name="ck_front_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="Rocker">
        <attr name="r_back_color" format="color"/>
        <attr name="r_front_color" format="color"/>
        <attr name="r_click_color" format="color"/>
        <attr name="r_type" format="integer"/>
    </declare-styleable>

    <declare-styleable name="SimpleKey">
        <attr name="sk_type" format="integer"/>
        <attr name="sk_back_color" format="color"/>
        <attr name="sk_font_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="Trigger">
        <attr name="t_type" format="integer"/>
        <attr name="t_begin_to_end" format="boolean"/>
        <attr name="t_back_color" format="color"/>
        <attr name="t_front_color" format="color"/>
        <attr name="t_font_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="AxisView">
        <attr name="av_back_color" format="color"/>
        <attr name="av_front_color" format="color"/>
        <attr name="av_type" format="integer"/>
    </declare-styleable>

</resources>